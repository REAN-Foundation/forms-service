{
    "name": "form-service",
    "version": "1.0.0",
    "description": "form-service",
    "main": "index.js",
    "scripts": {
      "test": "tsc && mocha -u tdd --timeout 999999 --colors ./dist/tests/api-tests/**/*.js",
      "test:watch": "mocha --require ts-node/register --timeout 30000 --watch tests/api-tests/tests/**/*.test.ts",
      "test:coverage": "nyc npm test",
      "build": "tsc",
        "start": "tsc && ts-node --files ./src/index.ts",
        "lint": "eslint ./src/**/*.ts",
        "lint:fix": "eslint ./src/**/*.ts --fix",
        "format": "prettier --write ./src/**/*.ts",
        "format:check": "prettier --check ./src/**/*.ts",
        "format:json": "prettier --write *.json",
        "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' ./src/index.ts",
        "docs": "node serve-docs.js"
      },
      "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/prashantkharade/project.git"
      },
      "author": "prashant",
      "license": "ISC",
      "bugs": {
        "url": "https://github.com/prashantkharade/project/issues"
      },
      "homepage": "https://github.com/prashantkharade/project#readme",
      "dependencies": {
        "@aws-sdk/client-s3": "^3.828.0",
        "@aws-sdk/s3-request-presigner": "^3.828.0",
        "@prisma/client": "^5.22.0",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "csv-writer": "^1.6.0",
        "dayjs": "^1.11.13",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-async-errors": "^3.1.1",
        "helmet": "^8.1.0",
        "joi": "^17.12.2",
        "moment-timezone": "^0.5.45",
        "mysql2": "^3.10.0",
        "pdfkit": "^0.15.0",
        "pg": "^8.16.0",
        "reflect-metadata": "^0.2.2",
        "tsyringe": "^4.8.0",
        "typeorm": "^0.3.24"
      },
      "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/chai": "^4.3.11",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.17",
        "@types/mime-types": "^2.1.4",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.3.1",
        "@types/pdfkit": "^0.13.4",
        "@types/pg": "^8.15.4",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "chai": "^4.3.10",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.5.1",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "prettier": "^3.6.2",
        "prisma": "^5.19.1",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
      },
      "keywords": []
}
