    "name": "form-service",
    "version": "1.0.0",
    "description": "form-service",
    "main": "index.js",
    "scripts": {
        "test": "tsc && mocha -u tdd --timeout 999999 --colors ./dist/tests/api-tests/**/*.js",
        "test:watch": "mocha --require ts-node/register --timeout 30000 --watch tests/api-tests/tests/**/*.test.ts",
        "test:coverage": "nyc npm test",
        "build": "tsc",
        "start": "tsc && ts-node --files ./src/index.ts",
        "lint": "eslint ./src/**/*.ts",
        "lint:fix": "eslint ./src/**/*.ts --fix",
        "format": "prettier --write ./src/**/*.ts",
        "format:check": "prettier --check ./src/**/*.ts",
        "format:json": "prettier --write *.json",
        "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' ./src/index.ts",
        "docs": "node serve-docs.js"
    },
