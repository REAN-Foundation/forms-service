meta {
  name: Delete Template Folder
  type: http
  seq: 5
}

delete {
  url: {{BASE_URL}}/template-folders/{{TEMPLATE_FOLDER_ID}}
  body: none
  auth: none
}

script:post-response {
  test("Request is successful", function () {
    expect(res.getStatus()).to.equal(200);
    var jsonRes = res.getBody();
    expect(jsonRes.Status).to.eql('Success');
  });
  
  test("Template Folder is deleted successfully", function () {
    var jsonRes = res.getBody();
    expect(jsonRes.Data).to.equal(true);
  });
}
