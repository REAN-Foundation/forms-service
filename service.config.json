
{
    "SystemIdentifier": "Kleo",
    "BaseUrl": "http://localhost:5555/api/v1",
    "Logging": {
        "Provider": "Custom",
        "Level": "Information"
    },
    "TemporaryFolders": {
        "UploadFolder": "./tmp/uploads/",
        "DownloadFolder": "./tmp/downloads/",
        "LogFolder": "./tmp/logs/",
        "CleanupEveryMinutes": 10
    },
    "FileStorage": {
        "Provider": "AWS-S3"
    },
    "Sms": {
        "Provider": "Twilio"
    },
    "Email": {
        "Provider": "SendGrid"
    },
    "MobileNotification": {
        "Provider": "Firebase"
    },
    "MaxUploadFileSize": 104857600,
    "Telemetry": false
}